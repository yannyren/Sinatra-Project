<h2>Student List</h2>
<a href="/students/new">Add New Student</a>

<br/>
<table>
  <tr>
    <th>Name</th>
    <th>Gender</th>
    <th>Cohort</th>
    <th>Company</th>
  </tr>
  <% for student in @students %>
  <tr>
    <td><%= student.name %></td>
    <td><%= student.gender %></td>
    <td><%= student.cohort %></td>
    <td><%= student.company %></td>
  </tr>
  <% end %>
</table>